{
  "name": "SimpleRN2",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "_comment": "update node_modules/react-native/scripts/react-native-xcode.sh to configure sourcemap",
    "build-ios": "cd ./ios && xcodebuild -scheme SimpleRN2",
    "bundle-ios": "react-native bundle --entry-file ./index.ios.js --platform ios --dev false --bundle-output ./bundle/main.jsbundle --sourcemap-output ./bundle/sourcemap.js --assets-dest ./bundle",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "react": "16.0.0-alpha.12",
    "react-native": "0.47.1",
    "react-navigation": "^1.0.0-beta.11"
  },
  "devDependencies": {
    "babel-jest": "20.0.3",
    "babel-preset-react-native": "2.1.0",
    "jest": "20.0.4",
    "react-test-renderer": "16.0.0-alpha.12"
  },
  "jest": {
    "preset": "react-native"
  }
}
